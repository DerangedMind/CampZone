<h1>Create a Group!</h1>

<%= form_for @group do |form| %>

  <%= form.label :name %>
  <%= form.text_filed :name %>

  <%= form.label :min_age %>
  <%= form.integer :min_age %>

  <%= form.label :max_age %>
  <%= form.integer :max_age %>

  <%= form.label :start_date %>
  <%= form.date :start_date %>

  <%= form.submit :Submit %>

<% end %>